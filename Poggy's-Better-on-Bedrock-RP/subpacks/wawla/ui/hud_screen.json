{

    "advancement_animation_in":{
      "anim_type":"offset",
      "easing":"linear",
      "duration":1.0,
      "from":[ "14% + 180px", 0 ],
      "to":[ 0, 0 ],
      "next":"@hud.advancement_animation_stay"
    },
    "advancement_animation_stay":{
      "anim_type":"wait",
      "duration":6.7,
      "next":"@hud.advancement_animation_out"
    },
    "advancement_animation_out":{
      "anim_type":"offset",
      "easing":"linear",
      "duration":1.0,
      "from":[ 0, 0 ],
      "to":[ "14% + 180px", 0 ],
      "destroy_at_end":"hud_actionbar_text"
    },
  /*
    "hud_actionbar_text":{
      "visible":"(($actionbar_text = 'lol') or ($actionbar_text = 'paper') or ($actionbar_text = 'first_steps') or ($actionbar_text = 'full_copper') or ($actionbar_text = 'pig_Quest_complete') or ($actionbar_text = 'hero_Quest_complete') or ($actionbar_text = 'village_Quest_complete') or ($actionbar_text = 'iron_Quest_complete'))"
    },
   */
  
   "anim_actionbar_text_alpha_out": {
    "anim_type": "alpha",
    "easing": "in_expo",
    "duration": 0.14,
    "from": 1.0,
    "to": 0,
    "destroy_at_end": "hud_actionbar_text"
  },
  "anim_actionbar_text_background_alpha_out": {
    "anim_type": "alpha",
    "easing": "in_expo",
    "duration": 0.14,
    "from": 1,
    "to": 0,
    "destroy_at_end": "hud_actionbar_text"
  },
  "auto_save_animation": {
    "anim_type": "flip_book",
    "initial_uv": [
      0,
      0
    ],
    "frame_count": 9,
    "frame_step": 18,
    "fps": 10,
    "reversible": true,
    "easing": "linear"
  },
  "auto_save": {
    "type": "image",
    "layer": 31,
    "texture": "textures/ui/auto_save",
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "offset": [
      -15,
      15
    ],
    "size": [
      18,
      30
    ],
    "uv_size": [
      18,
      30
    ],
    "uv": "@auto_save_animation",
    "disable_anim_fast_forward": true,
    "bindings": [
      {
        "binding_name": "#auto_save_animation_visible",
        "binding_name_override": "#visible"
      }
    ]
  },
  
